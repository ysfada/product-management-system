  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Go file",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${file}"
    },
    {
      "name": "Launch Go Package",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "args": ["-debug", "true"]
    }
  ]
